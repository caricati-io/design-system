  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/caricati-io"
  },
  "scripts": {
    "build": "rm -rf ./dist && BABEL_ENV=esm babel --source-maps --extensions \".js,.ts,.tsx\" src --out-dir dist/js",
    "test": "react-scripts test",
    "check": "CI=true npm test && npm run check:lint && npm run check:prettier && npm run check:types",
    "check:prettier": "prettier --check './src'",
    "check:lint": "eslint ./src",
    "check:types": "tsc --project ./tsconfig.json --noEmit",
    "lint": "eslint src",
    "eject": "react-scripts eject",
    "prepublishOnly": "npm run check && npm build",
    "start:demo": "start-storybook -p 6006 -s public",
    "build:storybook": "build-storybook"
  },
