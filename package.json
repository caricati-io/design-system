{
  "name": "@caricati/rui",
  "version": "0.1.0",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/caricati-io"
  },
  "scripts": {
    "build": "rm -rf ./dist && BABEL_ENV=esm babel --source-maps --extensions \".js,.ts,.tsx\" src --out-dir dist/js",
    "test": "jest ./src --env=jsdom",
    "test:cover": "jest --env=jsdom --coverage",
    "test:dev": "jest ./src --env=jsdom --coverage --verbose --watchAll",
    "check": "CI=true npm test && npm run check:lint && npm run check:types",
    "check:prettier": "prettier --list-different './src'",
    "check:lint": "eslint ./src",
    "check:types": "tsc --project ./tsconfig.json --noEmit",
    "lint": "eslint src",
    "prepublishOnly": "npm run check && npm build",
    "start:demo": "start-storybook -p 6006 -s public",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "dependencies": {
    "@fontsource/poppins": "^4.5.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "css-loader": "^6.7.3",
    "styled-components": "^5.3.6",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-react-jsx": "^7.20.13",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.65",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "babel-plugin-typescript-to-proptypes": "^2.0.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-styled-components-a11y": "^0.1.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "prettier": "2.7.1",
    "prop-types": "^15.8.1",
    "react-docgen-typescript-plugin": "^1.0.2",
    "webpack": "^5.74.0"
  }
}
